    <tools Inherit="False">
      <li>
        <label>left fist</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>22</power>
        <cooldownTime>2</cooldownTime>
        <armorPenetration>0.1</armorPenetration>
        <linkedBodyPartsGroup>LeftHand</linkedBodyPartsGroup>
        <surpriseAttack>
          <extraMeleeDamages>
            <li>
              <def>Stun</def>
              <amount>14</amount>
            </li>
          </extraMeleeDamages>
        </surpriseAttack>
      </li>
      <li>
        <label>right fist</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>16</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>RightHand</linkedBodyPartsGroup>
        <armorPenetration>0.1</armorPenetration>
        <surpriseAttack>
          <extraMeleeDamages>
            <li>
              <def>Stun</def>
              <amount>14</amount>
            </li>
          </extraMeleeDamages>
        </surpriseAttack>
      </li>
      <li>
        <label>teeth</label>
        <capacities>
          <li>Bite</li>
        </capacities>
        <power>12</power>
        <armorPenetration>0.1</armorPenetration>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>Teeth</linkedBodyPartsGroup>
        <chanceFactor>0.07</chanceFactor>
      </li>
      <li>
        <label>head</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>12</power>
        <armorPenetration>0.1</armorPenetration>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
        <ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
        <chanceFactor>0.2</chanceFactor>
      </li>
    </tools>
    <race>
      <lifeExpectancy>190</lifeExpectancy>
      <body>Namekian_Body</body>
      <baseBodySize>1</baseBodySize>
      <baseHealthScale>1</baseHealthScale>
      <baseHungerRate>1.5</baseHungerRate>
      <alwaysViolent>true</alwaysViolent>
    </race>

    <recipes>
    </recipes>

    <alienRace>
      <raceRestriction>
        <whiteGeneList>
        </whiteGeneList>
        <blackGeneList>
          <li>HairStyle</li>
          <li>BeardStyle</li>
        </blackGeneList>
        <onlyUseRaceRestrictedXenotypes>true</onlyUseRaceRestrictedXenotypes>
        <whiteXenotypeList>
          <li>SR_Namekian</li>
        </whiteXenotypeList>
        <xenotypeList>
          <li>SR_Namekian</li>
        </xenotypeList>
        <canReproduce>false</canReproduce>
        <blackEndoCategories>
          <li>HairColor</li>
        </blackEndoCategories>
      </raceRestriction>
      <generalSettings>
        <humanRecipeImport>true</humanRecipeImport>
        <allowHumanBios>true</allowHumanBios>
        <maleGenderProbability>1</maleGenderProbability>
        <alienPartGenerator Inherit="False">
          <bodyTypes>
            <li>Hulk</li>
            <li>Fat</li>
          </bodyTypes>
          <headTypes>
            <li>SR_NamekianHead</li>
          </headTypes>
          <bodyAddons>
            <li>
              <path>Namekian/Extra/NamekianAntenna</path>
              <alignWithHead>true</alignWithHead>
              <inFrontOfBody>true</inFrontOfBody>
              <colorChannel>skin</colorChannel>
              <userCustomizable>false</userCustomizable>
              <drawSize>(1.0, 1.0)</drawSize>
              <offsets>
                <south>
                  <offset>(0.0, 0.0)</offset>
                  <layerOffset>0.1</layerOffset>
                </south>
                <north>
                  <offset>(0.0, 0.15)</offset>
                  <layerOffset>-0.1</layerOffset>
                </north>
                <east>
                  <offset>(0.0, 0.0)</offset>
                  <layerOffset>0.1</layerOffset>
                </east>
              </offsets>
              <conditions>
                <BodyPart>
                  <bodyPartLabel>left antenna</bodyPartLabel>
                  <drawWithoutPart>false</drawWithoutPart>
                </BodyPart>
                <BodyPart>
                  <bodyPartLabel>right antenna</bodyPartLabel>
                  <drawWithoutPart>false</drawWithoutPart>
                </BodyPart>
              </conditions>
            </li>
          </bodyAddons>
          <colorChannels>
          <li>
            <name>skin</name>
            <first Class="ColorGenerator_Single">
              <color>RGBA(128, 255, 0, 255)</color>
            </first>
            <second Class="ColorGenerator_Single">
              <color>RGBA(255, 255, 255, 255)</color>
            </second>
          </li>
        </colorChannels>
        </alienPartGenerator>
        <reproduction>
          <maleFertilityAgeFactor>
            <points>
              <li>(14, 0)</li>
              <li>(18, 1)</li>
              <li>(70, 1)</li>
              <li>(90, 0)</li>
            </points>
          </maleFertilityAgeFactor>
          <femaleFertilityAgeFactor>
            <points>
              <li>(14, 0)</li>
              <li>(20, 1)</li>
              <li>(35, 1)</li>
              <li>(50, 0.5)</li>
              <li>(60, 0.1)</li>
              <li>(65, 0.02)</li>
              <li>(70, 0)</li>
            </points>
          </femaleFertilityAgeFactor>
        </reproduction>
      </generalSettings>
      <graphicPaths Inherit="False">
        <body>
          <path>Namekian/Body/Naked_Male</path>
        </body>
        <bodyMasks>
          <path>Namekian/BodyMasks/Naked_Male</path>
        </bodyMasks>
        <head>
          <path>Namekian/Head/Namekian_Head1</path>
        </head>
        <headMasks>
          <path>Namekian/HeadMasks/Namekian_Head1</path>
        </headMasks>
        <skinShader>Cutout</skinShader>
      </graphicPaths>
