    <description>Namekian.</description>
    <statBases>
      <MarketValue>4375</MarketValue>
      <PainShockThreshold>0.9</PainShockThreshold>
      <ArmorRating_Blunt>0.3</ArmorRating_Blunt>
      <ArmorRating_Sharp>0.3</ArmorRating_Sharp>
